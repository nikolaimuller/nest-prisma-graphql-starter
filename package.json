{
  "name": "nest-prisma-graphql-starter",
  "version": "0.0.0",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "dev": "yarn clean && load-env webpack",
    "build": "yarn clean && webpack --env build",
    "start": "node dist/server",
    "clean": "rimraf dist",
    "db:migrate": "load-env prisma db push",
    "db:seed": "load-env prisma db seed",
    "db:reset": "load-env prisma db reset",
    "format": "prettier --write \"src/**/*.ts\" \"prisma/**/*.ts\"",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint '{src,prisma}/**/*.{js,ts}' --cache",
    "fix-all-files": "yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "^2.2.1",
    "@nestjs/apollo": "^10.0.11",
    "@nestjs/common": "^8.4.5",
    "@nestjs/core": "^8.4.5",
    "@nestjs/graphql": "^10.0.11",
    "@nestjs/platform-express": "^8.4.5",
    "@prisma/client": "^3.14.0",
    "apollo-server-express": "^3.7.0",
    "bcrypt": "^5.0.1",
    "graphql": "16.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@mullerstd/eslint-config": "^2.0.1",
    "@mullerstd/load-env": "^0.0.3",
    "@mullerstd/prettier-config": "^1.0.2",
    "@nestjs/testing": "^8.4.5",
    "@types/bcrypt": "^5",
    "@types/express": "^4.17.13",
    "@types/faker": "5.5.3",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.34",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "faker": "5.5.3",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "rimraf": "^3.0.2",
    "run-script-webpack-plugin": "0.0.11",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
